gammesDiatoniquesOne = \relative c' { 
    \key c \major
    \time 4/4
    \omit TimeSignature
    c2 d | \noBreak e f | \noBreak g a | \noBreak b c | \noBreak d e | \noBreak f g | \noBreak a b | \noBreak c b | \noBreak
    a2 g | \noBreak f e | \noBreak d c | \noBreak b a | \noBreak g f | \noBreak e d | \noBreak c b | \noBreak c r |
    \bar "|."
}

gammesDiatoniquesSecondes = \relative c' { 
    \key c \major
